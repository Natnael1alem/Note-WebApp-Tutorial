<div class="container-fluid container-fluid-custom pd-5 mb-5">
    <!-- row-cols-2 row-cols-lg-5 g-2 g-lg-3 -->
    <div class="row">
        <% if(searchResults != '') { %>
            <% searchResults.forEach(function(note, index) { %>

                <a href="/dashboard/item/<%= note._id %>">
                    <h4><%= note.title %></h4>
                </a>

        <% })} else {%>
            <p>No Items Found.</p>
        <% } %>
    </div>
</div>